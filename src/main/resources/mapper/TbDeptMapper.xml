<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chenjx.office.api.mapper.TbDeptMapper">
    <select id="searchAllDept" resultType="java.util.HashMap">
        SELECT id,
               dept_name AS deptName
        FROM tb_dept
        ORDER BY id
    </select>
</mapper>
